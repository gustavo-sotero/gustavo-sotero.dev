# Configurações do Telegram Bot (obrigatórias para o formulário de contato)
TELEGRAM_BOT_TOKEN=seu_token_do_bot_aqui
TELEGRAM_CHAT_ID=seu_chat_id_aqui

# Configurações de ambiente
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
PORT=3000
HOSTNAME=0.0.0.0

# Configurações de Observabilidade
LOG_LEVEL=info
ENABLE_ANALYTICS=true
ENABLE_ERROR_TRACKING=true

# Vercel Analytics (opcional - configurado automaticamente no Vercel)
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=

# URLs para health checks externos (opcional)
# EXTERNAL_HEALTH_CHECK_URL=

# Configurações de CORS para métricas (opcional)
# ALLOWED_ORIGINS=https://yourdomain.com